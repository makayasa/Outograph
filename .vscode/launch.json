  "version": "0.2.0",
  "configurations": [
    {
      "name": "outograph",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "outograph (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "outograph (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "outograph (profile mode sksl)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": ["--cache-sksl", "--purge-persistent-cache"]
